#lang planet neil/sicp
(define (count-pairs x)
  (if (not (pair? x))
      0
      (+ (count-pairs (car x))
         (count-pairs (cdr x))
         1)))

(define a (list 'a 'b 'c))
(define b (cons (list 'a 'b 'c) '()))
(define c (list (list 'a 'b 'c) '() '() '()))
(count-pairs a)
(count-pairs b)
(count-pairs c)